PANEL_FIFO=/tmp/panel-fifo
case "$(hostname)" in
tini)
	PANEL_HEIGHT=24
	PANEL_FONT_SZ=16
	PANEL_TRAY_SPACE=11
	;;
deuce)
	PANEL_HEIGHT=32
	PANEL_FONT_SZ=22
	PANEL_TRAY_SPACE=15
	;;
esac
PANEL_WM_NAME=bspwm_panel
export PANEL_FIFO PANEL_HEIGHT PANEL_FONT_SZ PANEL_WM_NAME PANEL_TRAY_SPACE

